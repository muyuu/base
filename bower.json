{
  "name": "antibase",
  "version": "0.0.3",
  "repository": "https://github.com/muyuu/base",
  "ignore": [
    "**/.*",
    "node_modules"
  ],
  "dependencies": {
    "jquery": "~1",
    "underscore": "~1.7.0",
    "fontawesome": "~4.2.0"
  },
  "overrides": {
    "jquery": {
      "main": [
        "dist/jquery.min.js",
        "dist/jquery.min.map"
      ]
    },
    "underscore": {
      "main": [
        "underscore-min.js",
        "underscore-min.map"
      ]
    },
    "fontawesome": {
      "main": [
        "./css/font-awesome.min.css",
        "./fonts/*"
      ]
    }
  }
}
